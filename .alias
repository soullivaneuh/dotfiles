alias archive="arc archive"
alias unarchive="arc unarchive"
alias compress="arc compress"
alias extract="arc extract"
alias files="ranger"
alias f="files"
alias find="bfs"
alias https="http --default-scheme=https"
alias m="make"
alias mux="tmuxifier"
alias p="pwd"
alias gs="git status && echo && git brv"
alias gb="g brv"
alias gt="git tag | head"
# https://gitlab.com/nexylan/pretty
alias pretty='docker run --rm \
  --env IMAGE_TAG \
  --volume "${PWD}":"${PWD}" \
  --volume /var/run/docker.sock:/var/run/docker.sock \
  --workdir ${PWD} \
  registry.gitlab.com/nexylan/pretty:1.18.0'
